<table>
  <tr>
    <th> Album Title </th>
    <th> Genre </th>
    <th> Buy Price </th>
    <th> Sell Price </th>
  </tr>
  <% @album.each do |album| %>
    <tr>
    <td>  <%= album.title %> </td>
    <td>  <%= album.genre %> </td>
    <td>  <%= album.buy_price %> </td>
    <td>  <%= album.sell_price %> </td>
    <td><form method="post" action="/album/<%=album.id%>/delete">
      <input type = "submit" value = "Delete Album!">
    </form></td>
    </tr>
    <% end %>
</table>

<form method="get" action="/album/new">

<input type = "submit" value = "Create New Album!">
</form>
